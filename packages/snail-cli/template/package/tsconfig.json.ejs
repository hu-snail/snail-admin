{
  "extends": "../../tsconfig.json",
  <% if (style == 'compontent') { -%>
"compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsxImportSource": "vue",
    "jsx": "preserve",
    "isolatedModules": true,
    "allowJs": true
  },

  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "index.ts"]
  <% } else if (style == 'plugin') { -%>
"compilerOptions": {
    "outDir": "./lib"
  }
  <% } -%>
}
